<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maxim_ozarovskiy.ajtestapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/currency_exchange_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/currency_exchange_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TargetBaseCurrencyActivity"
            android:label="@string/currency_activity"/>
        <activity android:name=".activity.CompleteTickerActivity"
            android:label="@string/app_name"/>
        <activity android:name=".activity.HistoryActivity"
            android:label="@string/app_name"/>

    </application>

</manifest>